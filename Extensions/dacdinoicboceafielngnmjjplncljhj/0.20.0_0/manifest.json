{
   "background": {
      "persistent": true,
      "scripts": [ "lib/js/browser-polyfill.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "img/icon/icon128.png",
         "16": "img/icon/icon16.png",
         "48": "img/icon/icon48.png",
         "64": "img/icon/icon64.png"
      }
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "lib/js/browser-polyfill.js", "content.js", "content_alert.js", "content_popup.js", "content_modal.js", "content_sidemenu.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
   "default_locale": "pl",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "img/icon/icon128.png",
      "16": "img/icon/icon16.png",
      "48": "img/icon/icon48.png",
      "64": "img/icon/icon64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "permissions": [ "activeTab", "webNavigation", "cookies", "*://*.alerabat.com/", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.20.0",
   "web_accessible_resources": [ "img/*.png", "img/*.svg", "content.css", "fonts/*.woff2" ]
}
